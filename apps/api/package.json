{
  "name": "api",
  "version": "1.0.0",
  "main": "./dist/server.js",
  "prettier": "@pos/prettier-config",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsup",
    "dev": "pnpm env:load tsx watch src/server.ts | pino-pretty --colorize",
    "env:load": "dotenv -e ../../.env --",
    "lint": "pnpm env:load eslint --max-warnings 0",
    "lint:fix": "pnpm env:load eslint --fix",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/autoload": "^6.2.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.0",
    "@fastify/deepmerge": "^3.0.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.2",
    "@pos/i18n": "workspace:*",
    "@t3-oss/env-core": "^0.12.0",
    "accept-language-parser": "^1.5.0",
    "close-with-grace": "^2.2.0",
    "dotenv-cli": "catalog:",
    "fastify": "^5.2.1",
    "fastify-better-auth": "^1.0.1",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "^4.0.2",
    "pino-pretty": "^13.0.0",
    "use-intl": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@pos/eslint-config": "workspace:*",
    "@pos/prettier-config": "workspace:*",
    "@pos/tsconfig": "workspace:*",
    "@types/accept-language-parser": "^1.5.7",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "catalog:"
  }
}
