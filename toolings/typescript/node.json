{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node",
  "extends": "./base.json",
  "compilerOptions": {
    "declaration": false,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ESNext",
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "lib": ["esnext"],
    "types": ["node"]
  }
}
